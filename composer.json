{
    "name": "rollun-com/rollun-api",
    "description": "rollun-com/rollun-api",
    "type": "library",
    "license": "GPL-3.0",
    "keywords": [
        "php",
        "rest",
        "data store",
        "middleware"
    ],
    "homepage": "https://github.com/rollun-com/rollun-api",
    "support": {
        "issues": "https://github.com/rollun-com/rollun-api/issues",
        "source": "https://github.com/rollun-com/rollun-api"
    },
    "require": {
        "php": "^5.6 || ^7.0",
        "roave/security-advisories": "dev-master",
        "zendframework/zend-expressive": "^1.0",
        "zendframework/zend-expressive-helpers": "^2.0",
        "zendframework/zend-expressive-fastroute": "^1.0",
        "zendframework/zend-servicemanager": "^3.0",
        "zendframework/zend-stdlib": "^3",
        "zendframework/zend-db": "2.8.2",
        "zendframework/zend-filter": "^2.0.1",
        "zendframework/zend-expressive-twigrenderer": "^1.0",
        "psr/log": "^1.0",
        "rollun-com/rollun-installer": "~1.0.2",
        "zendframework/zend-http": "^2.5",
        "google/apiclient": "2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0",
        "squizlabs/php_codesniffer": "^2.3",
        "filp/whoops": "^1.1 || ^2.0"
    },
    "autoload": {
        "psr-4": {
            "rollun\\api\\": "src/",
            "rollun\\test\\api\\": "test/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "rollun\\api\\": "src/",
            "rollun\\test\\api\\": "test/src/"
        }
    },
    "suggest": {
    },
    "scripts": {
        "lib-install": "zaboy\\installer\\Commands::install",
        "lib-uninstall": "zaboy\\installer\\Commands::uninstall",
        "lib-reinstall": "zaboy\\installer\\Commands::reinstall"
    }

}
